{
  "name": "roadie",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "build:android": "cd android && ./gradlew build && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "debug:android": "cd android/ && ./gradlew assembleDebug && cd ..",
    "release:android": "cd android/ && ./gradlew assembleRelease && cd ..",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/Roadie/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.4",
